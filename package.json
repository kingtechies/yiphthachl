    "frontend"
  ],
  "author": "Yiphthachl Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yiphthachl/yiphthachl.git"
  },
  "homepage": "https://yiphthachl.dev",
  "bugs": {
    "url": "https://github.com/yiphthachl/yiphthachl/issues"
  },
  "files": [
    "dist",
    "compiler",
    "runtime",
    "ide",
    "README.md"
  ],
  "exports": {
